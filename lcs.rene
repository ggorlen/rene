function lcs(n: Int, m: Int, x: Array, y: Array):
    T = array_of_zeros(n, m)
    
    for i = 0 -> n:
        T[i][0] = 0
    
    for j = 0 -> m:
        T[0][j] = 0
    
    for i = 1 -> n:
        for j = 1 -> m:
            if x[i] == y[j]:
                T[i][j] = T[i-1][j-1] + 1
            else:
                T[i][j] = max(T[i-1][j], T[i][j-1])
    
    return T[n][m]

